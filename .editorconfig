; This file is for unifying the code style for different editors and IDEs. 
; More information at http://editorconfig.org 

root = true 

[*] 
charset = utf-8
indent-size = 4 
indent-style = 4
end_of_line = lf 
insert_final_newline = true
trim_trailing_whitespace = true 

[*.md]
trim_trailing_whitespace = false
